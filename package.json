{
  "name": "air-quality-monitoring-system",
  "version": "1.0.0",
  "description": "Comprehensive Air Quality Monitoring System with React Frontend and Node.js Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:server": "nodemon --config nodemon.json",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
  "diagnose:thingspeak": "node scripts/diagnose-thingspeak.js",
    "test": "jest --runInBand",
    "smoke": "node scripts/smoke-test.js"
  },
  "keywords": [
    "air-quality",
    "monitoring",
    "environmental",
    "sensors",
    "thingspeak",
    "lstm",
    "machine-learning",
    "react",
    "typescript"
  ],
  "author": "Air Quality Monitoring Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cache": "^5.1.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/air-quality-monitoring"
  },
  "bugs": {
    "url": "https://github.com/your-org/air-quality-monitoring/issues"
  },
  "homepage": "https://github.com/your-org/air-quality-monitoring#readme"
}
